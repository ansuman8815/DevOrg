    <webLinks>
        <fullName>Approve</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Approve</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/23.0/connection.js&quot;)}

var RequRecord = new sforce.SObject(&quot;PMCS__Request__c&quot;);
RequRecord .id=&quot;{!Task__c.Request_IDId__c }&quot;;

var TaskRecord = new sforce.SObject(&quot;PMCS__Task__c&quot;);

TaskRecord.id = &quot;{!Task__c.Id}&quot;;
TaskRecord.PMCS__Status__c = &quot;Approved&quot;;
RequRecord.PMCS__Status__c = &quot;Task Approved&quot;;

var result = sforce.connection.update([TaskRecord]);
var result1 = sforce.connection.update([RequRecord ]);

if (  (result[0].getBoolean(&quot;success&quot;) == true)  &amp;&amp;  (  (result1[0].getBoolean(&quot;success&quot;) == false) ||  (result1[0].getBoolean(&quot;success&quot;) ==true)  ) ){
window.location.reload();
alert(&quot;Your Task has been Approved&quot;);
}
else{
alert(&quot;You do not have permission to Approve this Task&quot;);
}</url>
    </webLinks>
