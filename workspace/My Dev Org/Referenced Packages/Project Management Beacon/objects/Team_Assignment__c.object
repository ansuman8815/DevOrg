    <validationRules>
        <fullName>Consumed_Over_Alotted_Time</fullName>
        <active>true</active>
        <errorConditionFormula>if( ISNEW() || ISCHANGED(Consumed_Time__c), 

if( ISPICKVAL( Consumed_over_Allotted_Time__c , &quot;Yes&quot;), (Assigned_Time__c + Tolerance__c) &lt;  Consumed_Time__c, 
if( ISPICKVAL(Consumed_over_Allotted_Time__c , &quot;No&quot;), Assigned_Time__c &lt;  Consumed_Time__c, null  )
),

null)</errorConditionFormula>
        <errorDisplayField>Consumed_Time__c</errorDisplayField>
        <errorMessage>Duration in hours must be less than Team assignment time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TA_End_Date_and_Task_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>( Task_ID__r.Task_End_Date__c &lt;  End_Date__c )</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>TA End Date must be less than Task End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TA_Start_Date_and_Task_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>( 
( Task_ID__r.Task_Start_Date__c  &gt;  Start_Date__c ) &amp;&amp; 
(  Task_ID__r.Task_End_Date__c &gt; Start_Date__c ) 
)</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>TeamAssignment Start Date must be less than Task Start Date and TeamAssignment Start Date must be less than Task End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_Rule_for_Assigned_Time</fullName>
        <active>false</active>
        <description>Assigned Time in Task and Assigned Time in Team Assignment</description>
        <errorConditionFormula>Task_ID__r.ConsumedTATime__c   &lt;  Assigned_Time__c</errorConditionFormula>
        <errorDisplayField>Assigned_Time__c</errorDisplayField>
        <errorMessage>Assigned Time in Team Assignment should not exceed Assigned Time in Task</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_for_Assigned_Time_from_Task</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
ISNEW() ,
Task_ID__r.Task_Assigned_Time__c &lt;  (Task_ID__r.ConsumedTATime__c + Assigned_Time__c) , 
Task_ID__r.Task_Assigned_Time__c &lt; (Task_ID__r.ConsumedTATime__c + 
( Assigned_Time__c - PRIORVALUE( Assigned_Time__c ))
)
)</errorConditionFormula>
        <errorDisplayField>Assigned_Time__c</errorDisplayField>
        <errorMessage>Assigned time for  this task is  exceeded</errorMessage>
    </validationRules>
