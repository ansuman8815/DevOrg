<aura:component >
    <aura:attribute name="label" type="String" />
    <aura:attribute name="value" type="String" />
    <div>
        <label class="uiLabel-top uiLabel">
            <span>{!v.label}</span>
        </label>
    </div>
    <input class="slds-input uiInput uiInputText" type="text" aria-describedby="" placeholder="" />
    <ui:outputRichText class="slds-input uiInput" value="{!v.value}" />
    <br/>
</aura:component>