<aura:component description="HcRelationshipEntityBlock" extends="HealthCloudGA:abstractRelationshipEntityBlock">

    <aura:set attribute="entityBlockAction">
        <HealthCloudGA:HcRelationshipEntityBlockActionButtons entity="{!v.entity}" actionTypes="{!v.actionTypes}"/>
    </aura:set>

    <aura:set attribute="entityNameLink">
        <HealthCloudGA:HcActionText label="{!v.entity.name}" objId="{!v.entity.objId}" />
    </aura:set>
</aura:component>