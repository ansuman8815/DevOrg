<aura:component >
    <aura:attribute name="class" type="String" access="public" />
    <aura:attribute name="style" type="String" access="public" />
    <aura:attribute name="svgPath" type="String" access="public" />
    <aura:attribute name="span_class" type="String" access="public"/>
    <span aura:id="svg_content"  class="{!v.span_class}">
    </span>
</aura:component>